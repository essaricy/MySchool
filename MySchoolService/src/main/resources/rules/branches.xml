<?xml version="1.0" encoding="UTF-8"?>
<rules>
	<field-sequence>
		<field name="BRANCH_CODE" mandatory="true" maxlength="8" datatype="ANY_CHARACTER" />
		<field name="DESCRIPTION" mandatory="true" maxlength="32" datatype="ANY_CHARACTER" />
		<field name="ADDRESS" mandatory="true" maxlength="128" datatype="ANY_CHARACTER" />
		<field name="REGION" mandatory="true" maxlength="64" datatype="ANY_CHARACTER" />
		<field name="PHONE_NUMBER" mandatory="true" maxlength="16" datatype="PHONE_NUMBER" />
		<field name="EMAIL_ID" mandatory="true" maxlength="32" datatype="EMAIL_ID" />
	</field-sequence>
</rules>