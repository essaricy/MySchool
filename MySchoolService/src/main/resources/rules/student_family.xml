<?xml version="1.0" encoding="UTF-8"?>
<rules>
    <field-sequence>
        <field name="ADMISSION_NUMBER" mandatory="true" maxlength="10" datatype="ANY_CHARACTER" />
        <field name="RELATIONSHIP" mandatory="true" maxlength="10" datatype="ANY_CHARACTER" />
        <field name="FAMILY_MEMBER_NAME" mandatory="true" maxlength="32" datatype="ANY_CHARACTER" />
        <field name="FAMILY_MEMBER_OCCUPATION" mandatory="false" maxlength="32" datatype="ANY_CHARACTER" />
        <field name="FAMILY_MEMBER_MOBILE_NUMBER" mandatory="false" maxlength="10" datatype="PHONE_NUMBER" />
        <field name="FAMILY_MEMBER_EMAIL_ID" mandatory="false" maxlength="32" datatype="EMAIL_ID" />
        <field name="AVAIL_SMS" mandatory="false" maxlength="3" datatype="YES_NO" />
        <field name="AVAIL_EMAIL" mandatory="false" maxlength="3" datatype="YES_NO" />
    </field-sequence>
</rules>