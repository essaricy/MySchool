<?xml version="1.0" encoding="UTF-8"?>
<rules>
    <field-sequence>
        <field name="BRANCH_CODE" mandatory="true" maxlength="8" datatype="ANY_CHARACTER" />
        <field name="DIVISION_CODE" mandatory="true" maxlength="8" datatype="ANY_CHARACTER" />
        <field name="SCHOOL_NAME" mandatory="true" maxlength="80" datatype="ANY_CHARACTER" />
        <field name="CLASS_NAME" mandatory="true" maxlength="16" datatype="ANY_CHARACTER" />
        <field name="MEDIUM_NAME" mandatory="true" maxlength="16" datatype="ANY_CHARACTER" />
        <field name="SECTION_NAME" mandatory="true" maxlength="16" datatype="ANY_CHARACTER" />
        <field name="ADMISSION_NUMBER" mandatory="true" maxlength="10" datatype="ANY_CHARACTER" />
        <field name="ADMISSION_STATUS" mandatory="true" maxlength="24" datatype="ANY_CHARACTER"  />
        <field name="DATE_OF_JOINING" mandatory="true" maxlength="10" datatype="DATE" />
        <field name="FIRST_NAME" mandatory="true" maxlength="16" datatype="NAME" />
        <field name="MIDDLE_NAME" mandatory="false" maxlength="16" datatype="NAME" />
        <field name="LAST_NAME" mandatory="true" maxlength="16" datatype="NAME" />
        <field name="GENDER" mandatory="true" maxlength="6" datatype="GENDER" />
        <field name="DATE_OF_BIRTH" mandatory="true" maxlength="10" datatype="DATE" />
        <field name="RELIGION" mandatory="false" maxlength="16" datatype="ANY_CHARACTER" />
        <field name="CASTE" mandatory="false" maxlength="16" datatype="ANY_CHARACTER" />
        <field name="NATIONALITY" mandatory="false" maxlength="8" datatype="ANY_CHARACTER" />
        <field name="MOTHER_TONGUE" mandatory="false" maxlength="20" datatype="ANY_CHARACTER" />
        <field name="PERMANENT_ADDRESS" mandatory="true" maxlength="128" datatype="ANY_CHARACTER" />
        <field name="CORRESPONDENCE_ADDRESS" mandatory="true" maxlength="128" datatype="ANY_CHARACTER" />
        <field name="MOBILE_NUMBER" mandatory="false" maxlength="10" datatype="PHONE_NUMBER" />
        <field name="IDENTIFICATION_MARKS" mandatory="false" maxlength="64" datatype="ANY_CHARACTER" />
        <field name="BLOOD_GROUP" mandatory="false" maxlength="8" datatype="ANY_CHARACTER" />
        <field name="REMARKS" mandatory="false" maxlength="128" datatype="ANY_CHARACTER" />
    </field-sequence>
</rules>