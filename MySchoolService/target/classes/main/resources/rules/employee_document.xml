<?xml version="1.0" encoding="UTF-8"?>
<rules>
    <field-sequence>
        <field name="EMPLOYEE_NUMBER" mandatory="true" maxlength="10" datatype="ANY_CHARACTER" />
        <field name="DOCUMENT_NAME" mandatory="true" maxlength="32" datatype="ANY_CHARACTER" />
        <field name="DOCUMENT_NUMBER" mandatory="false" maxlength="32" datatype="ANY_CHARACTER" />
        <field name="DOCUMENT_EXPIRY_DATE" mandatory="false" maxlength="10" datatype="DATE" />
