<?xml version="1.0" encoding="UTF-8"?>
<rules>
    <field-sequence>
        <field name="EMPLOYEE_NUMBER" mandatory="true" maxlength="10" datatype="ANY_CHARACTER" />
        <field name="DEGREE" mandatory="true" maxlength="32" datatype="ANY_CHARACTER" />
        <field name="SPECIALIZATION" mandatory="false" maxlength="32" datatype="ANY_CHARACTER" />
        <field name="YEAR_OF_GRADUATION" mandatory="true" maxlength="4" datatype="INTEGER" />
        <field name="COLLEGE" mandatory="true" maxlength="64" datatype="ANY_CHARACTER" />
        <field name="UNIVERSITY" mandatory="true" maxlength="64" datatype="ANY_CHARACTER" />
        <field name="PERCENTAGE" mandatory="true" maxlength="3" datatype="INTEGER" />
    </field-sequence>
</rules>