<?xml version="1.0" encoding="UTF-8"?>
<rules>
    <field-sequence>
        <field name="EMPLOYEE_NUMBER" mandatory="true" maxlength="10" datatype="ANY_CHARACTER" />
        <field name="EMPLOYER" mandatory="true" maxlength="64" datatype="ANY_CHARACTER" />
        <field name="JOB_TITLE" mandatory="true" maxlength="64" datatype="ANY_CHARACTER" />
        <field name="FROM_DATE" mandatory="true" maxlength="10" datatype="DATE" />
        <field name="TO_DATE" mandatory="true" maxlength="10" datatype="DATE" />
    </field-sequence>
</rules>