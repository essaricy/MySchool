<?xml version="1.0" encoding="UTF-8"?>
<rules>
    <field-sequence>
        <field name="EMPLOYEE_NUMBER" mandatory="true" maxlength="10" datatype="ANY_CHARACTER" />
        <field name="FIRST_NAME" mandatory="true" maxlength="16" datatype="NAME" />
        <field name="MIDDLE_NAME" mandatory="true" maxlength="16" datatype="NAME" />
        <field name="LAST_NAME" mandatory="true" maxlength="16" datatype="NAME" />
        <field name="GENDER" mandatory="true" maxlength="6" datatype="GENDER" />
        <field name="DATE_OF_BIRTH" mandatory="true" maxlength="10" datatype="DATE" />
        <field name="BLOOD_GROUP" mandatory="false" maxlength="8" datatype="ANY_CHARACTER" />
        <field name="NATIONALITY" mandatory="false" maxlength="8" datatype="ANY_CHARACTER" />
        <field name="MARITAL_STATUS" mandatory="true" maxlength="10" datatype="ANY_CHARACTER" />
        <field name="WEDDING_DAY" mandatory="false" maxlength="10" datatype="DATE" />
        <field name="EMPLOYED_AT" mandatory="true" maxlength="8" datatype="ANY_CHARACTER" />
        <field name="DESIGNATION_CODE" mandatory="true" maxlength="3" datatype="INTEGER" />
        <field name="EMPLOYEE_STATUS" mandatory="true" maxlength="24" datatype="ANY_CHARACTER" />
        <field name="EMPLOYMENT_START_DATE" mandatory="true" maxlength="10" datatype="DATE" />
        <field name="EMPLOYMENT_END_DATE" mandatory="false" maxlength="10" datatype="DATE" />
        <field name="REPORTING_TO" mandatory="false" maxlength="10" datatype="INTEGER" />
        <field name="REMARKS" mandatory="false" maxlength="512" datatype="ANY_CHARACTER" />
    </field-sequence>
</rules>